{
    "name": "gatsby-plugin-amp-fork",
    "version": "1.0.3",
    "description": "A gatsby plugin for scaffolding AMP pages",
    "main": "index.js",
    "scripts": {
        "build": "babel src --out-dir . --ignore **/__tests__",
        "prepare": "npm run build",
        "watch": "babel -w src --out-dir . --ignore **/__tests__",
        "commit": "git add . && git commit && git push -u origin master",
        "test": "jest"
    },
    "author": "Jonathan Faircloth",
    "license": "MIT",
    "homepage": "https://github.com/nonjene/gatsby-plugin-amp-fork#readme",
    "repository": "https://github.com/nonjene/gatsby-plugin-amp-fork",
    "bugs": {
        "url": "https://github.com/nonjene/gatsby-plugin-amp-fork/issues"
    },
    "dependencies": {
        "jsdom": "16.5.3",
        "lodash.flattendeep": "^4.4.0",
        "minimatch": "^3.0.4"
    },
    "peerDependencies": {
        "gatsby": "^2.32.0 || ^3.0.0 || ^4.0.0",
        "react": "^16.13.1 || ^17.0.2 || ^18.0.0"
    },
    "keywords": [
        "gatsby",
        "gatsby-plugin",
        "gatsby-plugin-amp",
        "amp",
        "accelerated-mobile-pages"
    ],
    "devDependencies": {
        "@babel/cli": "^7.13.16",
        "@babel/core": "^7.14.6",
        "@babel/preset-env": "^7.14.5",
        "@babel/preset-react": "^7.14.5",
        "@types/jest": "^26.0.23",
        "babel-jest": "^27.0.2",
        "babel-preset-gatsby-package": "1.4.0",
        "jest": "^27.0.4",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-test-renderer": "^17.0.2"
    }
}
